<Application x:Class="Illustro.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Illustro"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Theme/PrzyciskMenu.xaml"/>
                <ResourceDictionary Source="Theme/GłównaSekcja.xaml"/>
                <ResourceDictionary Source="Theme/TablicaHistoriaOdczytów.xaml"/>
                <ResourceDictionary Source="Theme/TablicaAktualnaWydajność.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
